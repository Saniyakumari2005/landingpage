        "#turn by turn message-level summary\n",
        "turn_summary = (\n",
        "    turn_df\n",
        "    .groupby([\"anonymous_user_id\", \"date\", \"message_id\"])\n",
        "    .agg(\n",
        "        sentiment_mean=(\"sentiment_strength\", \"mean\"),\n",
        "        sentiment_std=(\"sentiment_strength\", \"std\"),\n",
        "        polarity_mean=(\"polarity_score\", \"mean\"),\n",
        "        topic_drift_mean=(\"topic_drift_level\", \"mean\"),\n",
        "        pedagogical_adequacy_mean=(\"pedagogical_adequacy\", \"mean\")\n",
        "    )\n",
        "    .reset_index()\n",
        ")\n",
        "\n",
        "turn_summary.head()\n"
