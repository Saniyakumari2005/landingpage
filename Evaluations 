    {
      "cell_type": "code",
      "source": [
        "#Encode polarity numerically\n",
        "polarity_map = {\"negative\": -1, \"neutral\": 0, \"positive\": 1}\n",
        "\n",
        "conv_df[\"polarity_score\"] = conv_df[\"polarity\"].map(polarity_map)\n",
        "turn_df[\"polarity_score\"] = turn_df[\"polarity\"].map(polarity_map)"
      ],
      "metadata": {
        "id": "pMP7Ti7Co87S"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "markdown",
      "source": [
        "## 2 new combined/summarized dataframe"
      ],
      "metadata": {
        "id": "CYumZbn6xeJY"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "#conversation level summary\n",
        "conversation_summary = (\n",
        "    conv_df\n",
        "    .groupby([\"anonymous_user_id\", \"date\"])\n",
        "    .agg(\n",
        "        message_count=(\"message_count\", \"first\"),\n",
        "        sentiment_mean=(\"sentiment_strength\", \"mean\"),\n",
        "        sentiment_std=(\"sentiment_strength\", \"std\"),\n",
        "        confusion_mean=(\"confusion_level\", \"mean\"),\n",
        "        confusion_std=(\"confusion_level\", \"std\"),\n",
        "        engagement_mean=(\"engagement_level\", \"mean\"),\n",
        "        engagement_std=(\"engagement_level\", \"std\"),\n",
        "        polarity_mean=(\"polarity_score\", \"mean\")\n",
        "    )\n",
        "    .reset_index()\n",
        ")\n",
        "\n",
        "conversation_summary.head()\n"
      ],
