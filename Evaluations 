    {
      "cell_type": "code",
      "source": [
        "#combine all the conversations into these two rows\n",
        "conversation_rows = [] #data per conversation\n",
        "turn_rows = [] #data per message\n",
        "\n",
        "for file in json_files:\n",
        "#loop through all the JSON files and collect the data\n",
        "    with open(file, \"r\", encoding=\"utf-8\") as f:\n",
        "        data = json.load(f)\n",
        "\n",
        "    for user in data:\n",
        "        anon_id = user[\"anonymous_user_id\"]\n",
        "\n",
        "        for conv in user[\"analysed_conversations\"]:\n",
        "            # Conversation-level row\n",
        "            conversation_rows.append({\n",
        "                \"anonymous_user_id\": anon_id,\n",
        "                \"date\": conv[\"date\"],\n",
        "                \"message_count\": conv[\"message_count\"],\n",
        "                \"polarity\": conv[\"polarity\"],\n",
        "                \"sentiment_strength\": conv[\"sentiment_strength\"],\n",
        "                \"subjectivity\": conv[\"subjectivity\"],\n",
        "                \"confusion_level\": conv[\"confusion_level\"],\n",
        "                \"engagement_level\": conv[\"engagement_level\"],\n",
        "                \"motivation_level\": conv[\"motivation_level\"],\n",
        "                \"helpfulness_perception\": conv[\"helpfulness_perception\"],\n",
        "                \"politeness_respect\": conv[\"politeness_respect\"],\n",
        "                \"conflict_disagreement\": conv[\"conflict_disagreement\"],\n",
        "                \"resolution_detected\": conv[\"resolution_detected\"],\n",
        "                \"tone_evolution\": conv[\"tone_evolution\"],\n",
        "                \"topic_drift_level\": conv[\"topic_drift_level\"],\n",
        "                \"pedagogical_adequacy\": conv[\"pedagogical_adequacy\"]\n",
        "            })\n",
        "\n",
        "            # Turn-level rows\n",
        "            for turn in conv[\"turn_by_turn_sentiment\"]:\n",
        "                turn_rows.append({\n",
        "                    \"anonymous_user_id\": anon_id,\n",
        "                    \"date\": conv[\"date\"],\n",
        "                    \"message_id\": turn[\"message_id\"],\n",
        "                    \"polarity\": turn[\"polarity\"],\n",
        "                    \"sentiment_strength\": turn[\"sentiment_strength\"],\n",
        "                    \"emotion_category\": turn[\"emotion_category\"],\n",
        "                    \"topic_drift_level\": turn[\"topic_drift_level\"],\n",
        "                    \"pedagogical_adequacy\": turn[\"pedagogical_adequacy\"]\n",
        "                })\n",
        "\n",
        "#create two dataframes for each of the dataset\n",
        "conv_df = pd.DataFrame(conversation_rows)\n",
        "turn_df = pd.DataFrame(turn_rows)\n",
        "\n",
        "conv_df.head(), turn_df.head()\n",
        "\n"
      ],
